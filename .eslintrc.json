{
  "env": {
    "node": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "constructor-super": 1,
    "for-direction": 1,
    "getter-return": 1,
    "no-async-promise-executor": 1,
    "no-class-assign": 1,
    "no-compare-neg-zero": 1,
    "no-cond-assign": 1,
    "no-const-assign": 1,
    "no-constant-condition": 1,
    "no-control-regex": 1,
    "no-debugger": 1,
    "no-dupe-args": 1,
    "no-dupe-class-members": 1,
    "no-dupe-else-if": 1,
    "no-dupe-keys": 1,
    "no-duplicate-case": 1,
    "no-empty-character-class": 1,
    "no-empty-pattern": 1,
    "no-ex-assign": 1,
    "no-fallthrough": 1,
    "no-func-assign": 1,
    "no-import-assign": 1,
    "no-inner-declarations": 1,
    "no-invalid-regexp": 1,
    "no-irregular-whitespace": 1,
    "no-loss-of-precision": 1,
    "no-misleading-character-class": 1,
    "no-new-symbol": 1,
    "no-obj-calls": 1,
    "no-prototype-builtins": 1,
    "no-self-assign": 1,
    "no-self-compare": 1,
    "no-setter-return": 1,
    "no-sparse-arrays": 1,
    "no-this-before-super": 1,
    "no-undef": 1,
    "no-unexpected-multiline": 1,
    "no-unreachable": 1,
    "no-unsafe-finally": 1,
    "no-unsafe-negation": [1, { "enforceForOrderingRelations": true }],
    "no-unsafe-optional-chaining": 1,
    "no-unused-vars": 1,
    "no-useless-backreference": 1,
    "use-isnan": 1,
    "valid-typeof": 1,

    "camelcase": [1, { "properties": "never" }],
    "default-case-last": 1,
    "default-param-last": 1,
    "eqeqeq": 1,
    "func-style": [1, "declaration", { "allowArrowFunctions": true }],
    "max-classes-per-file": 1,
    "max-depth": 1,
    "max-lines": 1,
    "max-nested-callbacks": [1, 4],
    "max-params": [1, 4],
    "max-statements": [1, 100],
    "new-cap": [1, { "capIsNew": false }],
    "no-array-constructor": 1,
    "no-bitwise": [1, { "allow": ["<<", ">>"] }],
    "no-caller": 1,
    "no-case-declarations": 1,
    "no-delete-var": 1,
    "no-empty": 1,
    "no-empty-function": 1,
    "no-eval": 1,
    "no-extend-native": 1,
    "no-extra-bind": 1,
    "no-extra-boolean-cast": 1,
    "no-floating-decimal": 1,
    "no-global-assign": 1,
    "no-implied-eval": 1,
    "no-invalid-this": 1,
    "no-iterator": 1,
    "no-labels": 1,
    "no-lone-blocks": 1,
    "no-lonely-if": 1,
    "no-loop-func": 1,
    "no-multi-assign": 1,
    "no-multi-str": 1,
    "no-new": 1,
    "no-new-func": 1,
    "no-new-object": 1,
    "no-new-wrappers": 1,
    "no-octal": 1,
    "no-octal-escape": 1,
    "no-proto": 1,
    "no-regex-spaces": 1,
    "no-return-assign": 1,
    "no-return-await": 1,
    "no-sequences": 1,
    "no-shadow": [1, { "builtinGlobals": true, "hoist": "all" }],
    "no-shadow-restricted-names": 1,
    "no-throw-literal": 1,
    "no-undef-init": 1,
    "no-unneeded-ternary": [1, { "defaultAssignment": false }],
    "no-unused-expressions": 1,
    "no-useless-call": 1,
    "no-useless-catch": 1,
    "no-useless-computed-key": [1, { "enforceForClassMembers": true }],
    "no-useless-concat": 1,
    "no-useless-constructor": 1,
    "no-useless-escape": 1,
    "no-useless-rename": 1,
    "no-useless-return": 1,
    "no-var": 1,
    "no-void": 1,
    "no-with": 1,
    "object-shorthand": 1,
    "one-var": [1, "never"],
    "operator-assignment": 1,
    "prefer-arrow-callback": [1, { "allowUnboundThis": false }],
    "prefer-const": 1,
    "prefer-destructuring": 1,
    "prefer-exponentiation-operator": 1,
    "prefer-named-capture-group": 1,
    "prefer-numeric-literals": 1,
    "prefer-object-spread": 1,
    "prefer-promise-reject-errors": 1,
    "prefer-regex-literals": [1, { "disallowRedundantWrapping": true }],
    "prefer-rest-params": 1,
    "prefer-spread": 1,
    "prefer-template": 1,
    "radix": [1, "as-needed"],
    "require-await": 1,
    "require-yield": 1,
    "spaced-comment": [1, "always", { "line": { "markers": ["--", "*", "/*", "*/"] }, "block": { "balanced": true } }],
    "symbol-description": 1,
    "yoda": 1,

    "array-bracket-newline": [1, "consistent"],
    "array-bracket-spacing": 1,
    "array-element-newline": [1, "consistent"],
    "arrow-parens": 1,
    "arrow-spacing": 1,
    "block-spacing": 1,
    "brace-style": [1, "1tbs", { "allowSingleLine": true }],
    "comma-dangle": 1,
    "comma-spacing": 1,
    "comma-style": 1,
    "computed-property-spacing": 1,
    "dot-location": [1, "property"],
    "eol-last": 1,
    "func-call-spacing": 1,
    "function-call-argument-newline": [1, "consistent"],
    "function-paren-newline": [1, "multiline-arguments"],
    "generator-star-spacing": [1, { "before": false, "after": true, "anonymous": "neither", "method": "both" }],
    "indent": [1, 2, { "SwitchCase": 1, "MemberExpression": 0, "flatTernaryExpressions": true }],
    "key-spacing": 1,
    "keyword-spacing": 1,
    "line-comment-position": [1, { "ignorePattern": "--" }],
    "linebreak-style": 1,
    "lines-between-class-members": [1, "always", { "exceptAfterSingleLine": true }],
    "max-len": [1, { "code": 200 }],
    "max-statements-per-line": 1,
    "new-parens": 1,
    "newline-per-chained-call": [1, { "ignoreChainWithDepth": 3 }],
    "no-extra-parens": [1, "all", { "enforceForArrowConditionals": false }],
    "no-multi-spaces": 1,
    "no-multiple-empty-lines": [1, { "max": 1, "maxEOF": 0, "maxBOF": 0 }],
    "no-tabs": 1,
    "no-trailing-spaces": 1,
    "no-whitespace-before-property": 1,
    "nonblock-statement-body-position": [1, "beside", { "overrides": { "if": "any", "for": "below" } }],
    "object-curly-newline": 1,
    "object-curly-spacing": [1, "always"],
    "object-property-newline": [1, { "allowAllPropertiesOnSameLine": true }],
    "operator-linebreak": [1, "before"],
    "padded-blocks": [1, "never"],
    "padding-line-between-statements": [
      1,
      { "blankLine": "always", "prev": ["function", "class", "import", "export"], "next": "*" },
      { "blankLine": "always", "prev": "*", "next": ["function", "class", "import", "export"] },
      { "blankLine": "any", "prev": "export", "next": "export" },
      { "blankLine": "never", "prev": ["import", "const", "let"], "next": ["import", "const", "let"] }
    ],
    "rest-spread-spacing": 1,
    "semi": [1, "never"],
    "space-before-blocks": 1,
    "space-before-function-paren": [1, { "anonymous": "never", "named": "never", "asyncArrow": "always" }],
    "space-in-parens": 1,
    "space-infix-ops": 1,
    "space-unary-ops": 1,
    "switch-colon-spacing": 1,
    "template-curly-spacing": 1,
    "template-tag-spacing": 1,
    "unicode-bom": 1,
    "wrap-iife": [1, "any"],
    "yield-star-spacing": 1
  }
}
